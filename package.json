{
  "name": "redux-tools",
  "version": "0.8.1",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/lundegaard/redux-tools",
  "contributors": [
    "Tomas Konrady <tomas.konrady@lundegaard.eu>",
    "Vaclav Jancarik <vaclav.jancarik@lundegaard.eu>"
  ],
  "engines": {
    "node": ">=8"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.5.0",
    "eslint-config-react-union": "^0.7.2",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "1.1.2",
    "invariant": "^2.2.4",
    "jest": "^23.6.0",
    "lerna": "2.2.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "ramda": "^0.25.0",
    "ramda-extension": "^0.6.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "scripts": {
    "publishAll": "lerna publish",
    "test": "jest",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint --ext .js ./",
    "build": "lerna exec -- rollup -c=../../rollup.config.js",
    "prepublish": "yarn build"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier --write",
        "yarn lint --fix",
        "git add"
      ]
    }
  }
}
